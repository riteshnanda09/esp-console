%table#app-crashes-table{:class => "table table-bordered table-hover"}
  %thead
    %tr
      %th.thsort{:scope => "col"}=t('apps.crashes.instance_id_head')
      %th.thsort{:scope => "col"}=t('apps.crashes.crashed_time_head')
  %tbody
    = render :partial => "apps/crashes/app_crash_item", :collection => @app[:crashes]